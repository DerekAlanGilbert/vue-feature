{
  "name": "@derekalangilbert/vue-feature",
  "version": "1.0.2",
  "main": "dist/vueFeature.common.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "url": "git://github.com/derekalangilbert/vue-feature.git"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
